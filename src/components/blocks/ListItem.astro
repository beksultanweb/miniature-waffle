---
interface Props {
  children: {
    children: {
      type: string;
      text: string;
    }[];
  }[];
}

const { children } = Astro.props;
---

<ul
  class="container mx-auto mt-4 sm:mt-6 pr-5 pl-10 flex flex-col gap-2.5 list-disc"
>
  {
    children.map((child) => (
      <li class="text-base sm:text-28 leading-130% font-normal">
        {child.children[0].text}
      </li>
    ))
  }
</ul>
